window.adblock=false;var adbCheck=function(){document.body.insertAdjacentHTML("beforeend","<div id='fake' aria-hidden='true'><div class='ad_box ad-richmedia-overlay' id='fake_adv'></div><div class='share_box' id='fake_soc'></div><div class='subscribe-form' id='fake_ang'></div><div class='adb-enabled' id='fake_ant'></div><div id='fake_scm' class='remodal' data-remodal-options=\"hashTracking: false\" data-remodal-id=\"login\"></div></div>");var fake=document.getElementById("fake");window.getComputedStyle(fake).getPropertyValue("visibility");var check=function(id){return(window.getComputedStyle(document.getElementById(id)).getPropertyValue("display")==="none");};window.setTimeout(function(){document.getElementById("fake").remove();},1500);return{ads_block:check("fake_adv"),soc_block:check("fake_soc"),ang_block:check("fake_ang"),ant_block:check("fake_ant"),scm_block:check("fake_scm")};};var result=adbCheck();Object.keys(result).map(function(key,i){if(key==='ads_block'&&result[key]){$(function(){$.cookie('acloseone','1',{expires:-1,'path':'/'});console.log($.cookie('aclosenew'));if($.cookie('aclosenew')===undefined){$('.adBl').slideDown('1000');}});}});